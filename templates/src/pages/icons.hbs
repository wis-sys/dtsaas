    <h3 class="title-hero">
        Skycons icons
    </h3>
    <p class="title-lead">Use the markup below to create Skycons canvas icons.</p>

    <div class="row mrg20T remove-production">
        <div class="col-md-6">
            <div class="content-box">
                <h3 class="content-box-header bg-default">
                    <i class="glyph-icon icon-code-fork"></i>
                    Dependencies
                </h3>
                <div class="content-box-wrapper">
                    <h4 class="text-transform-upr font-gray font-size-16">Javascript</h4>
                    <ul>
                        <li>
                            Skycons - <b>/assets/widgets/skycons/skycons.js</b>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-md-6 text-center">
            <div class="alert alert-warning mrg15B">
                <p>To see all the available options &amp; configurations, we suggest reading the Skycons external documentation accessible through the link below.</p>
            </div>
            <a href="http://darkskyapp.github.io/skycons/" title="Skycons documentation" target="_blank" class="btn display-inline btn-success">
                <i class="glyph-icon icon-external-link"></i>
                Skycons external documentation
            </a>
        </div>
    </div>

    <div class="example-box-wrapper">

        <canvas id="clear-day" width="64" height="64"></canvas>
        <canvas id="clear-night" width="64" height="64"></canvas>
        <canvas id="partly-cloudy-day" width="64" height="64"></canvas>
        <canvas id="partly-cloudy-night" width="64" height="64"></canvas>
        <canvas id="cloudy" width="64" height="64"></canvas>
        <canvas id="rain" width="64" height="64"></canvas>
        <canvas id="sleet" width="64" height="64"></canvas>
        <canvas id="snow" width="64" height="64"></canvas>
        <canvas id="wind" width="64" height="64"></canvas>
        <canvas id="fog" width="64" height="64"></canvas>

		<div class="example-html">
			<pre>
				<code class="language-markup">

                    <canvas id="clear-day" width="64" height="64"></canvas>
                    <canvas id="clear-night" width="64" height="64"></canvas>
                    <canvas id="partly-cloudy-day" width="64" height="64"></canvas>
                    <canvas id="partly-cloudy-night" width="64" height="64"></canvas>
                    <canvas id="cloudy" width="64" height="64"></canvas>
                    <canvas id="rain" width="64" height="64"></canvas>
                    <canvas id="sleet" width="64" height="64"></canvas>
                    <canvas id="snow" width="64" height="64"></canvas>
                    <canvas id="wind" width="64" height="64"></canvas>
                    <canvas id="fog" width="64" height="64"></canvas>

                    <script type="text/javascript">
                        $(function() { "use strict";
                            var icons = new Skycons(),
                                list  = [
                                    "clear-day", "clear-night", "partly-cloudy-day",
                                    "partly-cloudy-night", "cloudy", "rain", "sleet", "snow", "wind",
                                    "fog"
                                ],
                                i;
                            for(i = list.length; i--; )
                                icons.set(list[i], list[i]);
                            icons.add(document.getElementById("icon-cloud"), Skycons.PARTLY_CLOUDY_DAY);
                            var skycons2 = new Skycons({"color": "white"});
                            skycons2.add(document.getElementById("icon-clear-2"), Skycons.PARTLY_CLOUDY_NIGHT);
                            icons.play();
                        });
                    </script>

				</code>
			</pre>
		</div>
        {{> docs-buttons  }}
    </div>

